# KIS API Configuration (선택 사항 - 사용자별 키 사용 권장)
# Get your API keys from https://apiportal.koreainvestment.com/

# Application Key (발급받은 앱 키)
APP_KEY=your_app_key_here

# Application Secret (발급받은 앱 시크릿)
APP_SECRET=your_app_secret_here

# Account Number (계좌번호 8자리)
ACCOUNT_NO=12345678

# Account Product Code (계좌 상품 코드 2자리)
ACNT_PRDT_CD=01

# Is Simulation Mode (모의투자: true, 실전투자: false)
IS_SIMULATION=true

# JWT Authentication
# JWT 토큰 생성에 사용되는 비밀키 (반드시 변경하세요!)
SECRET_KEY=your-secret-key-change-this-in-production-use-random-string

# Access Token 만료 시간 (분 단위)
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Encryption
# 사용자 API Key 암호화에 사용되는 키 (Fernet 키)
# 생성 방법: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
ENCRYPTION_KEY=your-fernet-encryption-key-here
